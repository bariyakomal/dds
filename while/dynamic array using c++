#include <iostream>
using namespace std;

int main() {
  int n;
  cout<<"enter the size ofarray:";
  cin>>n;
  int *arr=new int[n];
  for(int i=0; i<n; i++){
  cout<<"enter thge value of arr["<<i<<"]index: ";
  cin>>arr[i];
  }
  cout<<endl<<"entered element are:"<<endl;
  for(int i=0;i<n;i++){
      cout<<arr[i]<<endl;
  }
  
  

}
